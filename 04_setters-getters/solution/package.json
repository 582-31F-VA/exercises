{
  "name": "solution",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "dprint": "^0.49.1",
    "oxlint": "^0.16.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "lint": "bun install && bun oxlint",
    "dev": "bun server.ts 2>/dev/null || bun index.html",
    "fmt": "bun install && bun dprint fmt",
    "typecheck": "bun tsc --noEmit",
    "submit": "bun fmt && git add . && git commit -m submit && git push"
  }
}
